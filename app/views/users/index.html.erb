<div class="container">
<div class="row">
<div class="col">
<% if logged_in? %>
<ul>
<li>login中</li>
<li><%= link_to "ログアウト", logout_path, method: :post %></li>
</ul>
<table class="table">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Name</th>
      <th scope="col">Email</th>
    </tr>
  </thead>
  <tbody>
	<%= render partial: 'user', collection: @users %>
  </tbody>
</table>
<% else %>
<%= link_to "ログイン", login_path %>
<%= link_to "新規登録はこちら", new_user_path %>
<% end %>
</div>
</div>
</div>
