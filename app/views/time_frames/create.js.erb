$(function(){
	var time_frame = document.createTextNode("<%= @time_frame.time_frame.strftime("%m/%d %H時%M分")%>");
	var delete_link = document.createTextNode("<%= link_to "削除", time_frame_path(@time_frame), class: "btn btn-danger", method: :delete %>");
	var tr = document.createElement("tr");
	var td = document.createElement("td");
	var tdchild = document.createElement("td");
	td.appendChild(time_frame);
	tdchild.appendChild(delete_link);
	tr.appendChild(td);
	tr.appendChild(tdchild);
	$("#js-user_time_frame").append(tr);
});